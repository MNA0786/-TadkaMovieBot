{
    "name": "tadkamoviebot/telegram-bot",
    "description": "Telegram Movie Bot with Google Drive integration",
    "type": "project",
    "require": {
        "php": ">=8.1.0",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 300
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}